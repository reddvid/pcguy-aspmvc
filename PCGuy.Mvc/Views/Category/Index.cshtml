@using PCGuy.Common.Entities
@model List<Category>

@{
    ViewData["Title"] = "Categories";
}

<h1>Categories</h1>

<table class="table table-bordered table-striped">
    <thead>
    <tr>
        <th>Name</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var category in Model)
    {
        <tr>
            <td>@category.Name</td>
        </tr>
    }
    </tbody>
</table>